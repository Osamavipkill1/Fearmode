-- entity config
local looktime = "0.02"
local killmsg = {"..."}
onNotLook = function()
	if game.Players.LocalPlayer.Character.Humanoid.Health >= 10 then
    game.Players.LocalPlayer.Character.Humanoid.Health = game.Players.LocalPlayer.Character.Humanoid.Health - 5
	
    -- LAGGER --
        local FPS = 7
    local clock = tick()
        while clock + 1 / FPS > tick() do end
    wait()
    clock = tick()
    end
end
--
local lastroom = game:GetService("ReplicatedStorage").GameData.LatestRoom.Value
-- does the room exist???
local exist = game:GetService("Workspace").CurrentRooms:FindFirstChild(lastroom)
--
-- essentials-------------------------------
local Players = game:GetService("Players")

local Plr = Players.LocalPlayer
local Char = Plr.Character or Plr.CharacterAdded:Wait()
local Hum = Char:WaitForChild("Humanoid")
local Camera = workspace.CurrentCamera
local WorldToViewportPoint = Camera.WorldToViewportPoint

local SelfModules = {
    Functions = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Functions.lua"))(),
}
 
-- Load a custom instance model from a URL or local file
--
local entityModel = SelfModules.Functions.LoadCustomInstance("https://github.com/PABMAXICHAC/doors-monsters-models/blob/main/hunger.rbxm?raw=true")
if typeof(entityModel) == "Instance" and entityModel.ClassName == "Model" then
    entityModel.PrimaryPart = entityModel.PrimaryPart or entityModel:FindFirstChildWhichIsA("BasePart")
 
    if entityModel.PrimaryPart then
        -- Position
        if exist then
        entityModel.PrimaryPart.Position = game:GetService("Workspace").CurrentRooms[lastroom].Parts.Floor.Position
        entityModel.PrimaryPart.Position = entityModel.PrimaryPart.Position + Vector3.new(0, 5, 0)
        else
        entityModel.PrimaryPart.Position = game:GetService("Workspace").Lobby.Parts:GetChildren()[122].Position
        entityModel.PrimaryPart.Position = entityModel.PrimaryPart.Position + Vector3.new(0, 5, 0)
        end
        -- Set the parent of the model to game.Workspace
        entityModel.Parent = game.Workspace
 
        entityModel.PrimaryPart.Anchored = true
 
        -- Set the name of the model
        if entityModel.Name then
            entityModel.Name = "Hunger"
        end

        entityModel:SetAttribute("IsCustomEntity", true)
        entityModel:SetAttribute("NoAI", false)
        entityModel.HungerNew.Attachment.ParticleEmitter.Rate = "50"
    end
end
--
------------------------------------
-- some cool functions haha !11!1!1!!1
-- turns into nightmare
        game.Workspace.Hunger.HungerNew.Attachment.ParticleEmitter.Color = ColorSequence.new({
            ColorSequenceKeypoint.new(0, Color3.new(0, 0, 1)),
            ColorSequenceKeypoint.new(0, Color3.new(0, 0, 1)),
            ColorSequenceKeypoint.new(1, Color3.new(0, 0, 1))
        })
--
game.Workspace.Hunger.HungerNew.Attachment.ParticleEmitter.Rate = "100"
game.Workspace.Hunger.HungerNew.Attachment.PointLight.Color = Color3.fromRGB(0, 0, 255)
--
-- on look

--
function touch()
    local plr_hum = game.Players.LocalPlayer.Character.Humanoid
    if plr_hum then
        plr_hum.Health = 0
        local plr = game:GetService("Players").LocalPlayer
        game.ReplicatedStorage.GameStats["Player_".. plr.Name].Total.DeathCause.Value = "Hunger (Touched Entity)"
        --TRUE CRASH GAME--
    entityModel.HungerNew.crash3.Volume = "20"
    entityModel.HungerNew.crash3.Looped = false
    entityModel.HungerNew.crash3:Play()

                local FPS = 0
    local clock = tick()
        while clock + 1 / FPS > tick() do end
    wait()
    clock = tick()
-----------------------------------------------------
        firesignal(game.ReplicatedStorage.Bricks.DeathHint.OnClientEvent, killmsg)
    end
end
--

-- run functions !!!!!!!!!!!!!!!!!!!!!!!!!
entityModel.HungerNew.Touched:Connect(touch)
--

                -- Look at entity
while lastroom == game:GetService("ReplicatedStorage").GameData.LatestRoom.Value do
wait(looktime)
                local _, onScreen = WorldToViewportPoint(Camera, entityModel.PrimaryPart.Position)

                if not onScreen and game.Players.LocalPlayer.Character.Humanoid.Health > 0 then
                    task.spawn(onNotLook)
        if game.Players.LocalPlayer.Character.Humanoid.Health >= 6 then
                        local plr = game:GetService("Players").LocalPlayer
                        game.ReplicatedStorage.GameStats["Player_".. plr.Name].Total.DeathCause.Value = entityModel.Name
                        firesignal(game.ReplicatedStorage.Bricks.DeathHint.OnClientEvent, killmsg)
                entityModel.HungerNew.crash.Volume = "10"
                entityModel.HungerNew.crash:Play()
                else
                entityModel.HungerNew.crash:Stop()
                end
        end
                
end
--

game:GetService("Workspace").Hunger:Destroy()
--

--
wait(0.1)
--health divider
local dividenumber = math.random(2, 4)
if game.Players.LocalPlayer.Character.Humanoid.Health > 6 then
local hp_hp = game.Players.LocalPlayer.Character.Humanoid.Health
game.Players.LocalPlayer.Character.Humanoid.Health = hp_hp / dividenumber
end
--
local hp_hp2 = game.Players.LocalPlayer.Character.Humanoid.Health -- store health
wait(0.1)
if hp_hp2 <= 5 then
game.Players.LocalPlayer.PlayerGui.MainUI.MainFrame.Healthbar.Bar.Bar.BackgroundColor3 = Color3.fromRGB(0, 0, 255)
print("STATIC HEALTH ENABLED")
end
-- stop health updates
if hp_hp2 <= 5 then
while true do
wait(0.01)
game.Players.LocalPlayer.Character.Humanoid.Health = hp_hp2
--
if game:GetService("Workspace"):FindFirstChild("WH1T3") ~= nil then
game.Players.LocalPlayer.PlayerGui.MainUI.MainFrame.Healthbar.Bar.Bar.BackgroundColor3 = Color3.fromRGB(255, 222, 189)
break
end
--
end
end
