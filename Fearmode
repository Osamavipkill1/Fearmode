coroutine.wrap(function()
while true do
local fbsz = game:GetService("Players").LocalPlayer.PlayerGui.FearScreen.Fearbar.Bar.Bar
local fear = fbsz.Size.X.Scale * 100
if fear >= 20 and fear <= 40 then
local f2040 = math.random(70, 100)
wait(f2040)
game.ReplicatedStorage.GameData.LatestRoom.Changed:Wait()
loadstring(game:HttpGet("https://github.com/TheendisnearDonotfear/FearmodeDoors/raw/main/eyesop"))()
elseif fear >= 41 and fear <= 75 then
local f4175 = math.random(50, 80)
wait(f4175)
game.ReplicatedStorage.GameData.LatestRoom.Changed:Wait()
loadstring(game:HttpGet("https://github.com/TheendisnearDonotfear/FearmodeDoors/raw/main/eyesop"))()
elseif fear >= 76 and fear <= 100 then
local f76100 = math.random(20,60)
wait(f76100)
game.ReplicatedStorage.GameData.LatestRoom.Changed:Wait()
loadstring(game:HttpGet("https://github.com/TheendisnearDonotfear/FearmodeDoors/raw/main/eyesop"))()
elseif fear <= 19 then
wait(25)
print("op eyes didnt spawn.")
end
end
end)()
