-- random ambience
coroutine.wrap(function()
while true do
wait(150, 900)
--
local bushe = function()
local bush = game.Workspace.Ambience_Ambush
bush.Volume = 1.25
bush:Play()
end
local rushe = function()
local rush = game.Workspace.Ambience_Rush
rush.Volume = 5
rush:Play()
end
local rushee = function()
local rusheee = game.Workspace.Ambience_RushTease
rusheee.Volume = 5
rusheee:Play()
end
local ara = math.random(1,3)
if ara == 1 then
bushe()
elseif ara == 2 then
rushe()
elseif ara == 3 then
rushee()
end
end
end)()
